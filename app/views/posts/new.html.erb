
<%= render 'shared/content_lead', header: 'Create Your Post' %>

<%= form_for @post do |f| %>
<%= render 'shared/errors', obj: @post %>
	<%= f.label :title %>
	<%= f.text_field :title %> <br />
	<%= f.label :description %>
	<%= f.text_area :description, :size => "30x8" %> <br />
	<%= f.label :url %>
	<%= f.text_field :url %> <br />
  <%= f.submit "Create", class:"btn" %> <br />
<% end %>